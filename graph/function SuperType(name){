function SuperType(name){
	this.name = name;
}

SuperType.prototype.getSuperName = function(){
	return this.name + "-" + "OK."
}

SubType.prototype = new SuperType("Saumya")
function SubType(age){
	this.age = age;
}

SubType.prototype.getSubAge = function(){
	return this.age;
}

subtype1 = new SubType(26)
subtype1.name
subtype1.age
subtype1.getSuperName()
subtype1.getSubAge()


subtype3 = new SubType(39)
subtype3.name
subtype3.age
subtype3.getSuperName()
subtype3.getSubAge()

